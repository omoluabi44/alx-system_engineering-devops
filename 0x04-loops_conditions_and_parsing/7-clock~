#!/usr/bin/env bash
# This script displays the time for 12 hours and 59 minutes

# Display hours from 0 to 12


hour=0

while [ $hour -le 12 ]
do
    echo "Hour: $hour"
    ((hour++))
done

hour=0
while [ $hour -le 12 ]
do
    minute=1
    while [ $minute -le 59 ]
    do
	echo "$minute"
	((minute++))
    done
    ((hour++))
done
